<style>
    .tablist {
        width: 100%;
        height: 30px;
        border-bottom: #ddd 1px solid;
    }
    .btn-actor, .btn-rights{
        width: auto;
        height: 30px;
        padding: 0 10px;
        text-align: center;
        line-height: 30px;
        color: #fff;
        border: 1px solid transparent;
        border-radius: 3px;
        background: #ddd;

    }
    .btn-current {
        background: #70bc67;

    }
    .tabs table td,.tabs table th{

        border: #ddd 1px solid !important;
