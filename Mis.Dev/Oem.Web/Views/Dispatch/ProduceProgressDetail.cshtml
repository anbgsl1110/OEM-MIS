}

<style>
    .tablist {
        width: 100%;
        height: 30px;
        border-bottom: #ddd 1px solid;
        margin-bottom: 10px;
    }
    .btn-actor{
        width: auto;
        height: 30px;
        padding: 0 10px;
        text-align: center;
        line-height: 30px;
        color: #fff;
        border: 1px solid transparent;
        border-radius: 3px;
        background: #ddd;

    }
    .btn-current {
        background: #70bc67;

    }
    .tabs table td,.tabs table th{

        border: #ddd 1px solid !important;
        text-align: center;
        padding: 0 10px;
    }
    .ListDate td {
        height: 30px;
    }
    .consult-content .btn-box {

        border-top: 0px solid #fff;
        padding-top: 0px;
    }

    .buttons {
        height: 30px;
    }
    
    .buttons div {
        float: left;
        line-height: 30px;
        margin-right: 10px;
        cursor: pointer;

    }
</style>

